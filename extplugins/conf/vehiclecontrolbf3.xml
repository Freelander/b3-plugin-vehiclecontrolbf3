<configuration plugin="vehiclecontrolbf3">
	<settings name="messages">
		<set name="no_vehicle_respawn">Combat vehicles will NOT respawn on this map! Use them wisely.</set>
	</settings>
	<disable_vehicle_respawn>
		<!-- 
		For all the maps you enter below, vehicles will spawn only once at the beginning of round and will 
		not respawn again if vehicle spawn enabled in the config file.
		-->
		<map>MP_001</map>
		<map>MP_003</map>
	</disable_vehicle_respawn>
</configuration>